{
  "extends": "../tsconfig.json",
  "compilerOptions": {
    "baseUrl": "",
    "declaration": false,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "lib": [
      "es2018",
      "dom"
    ],    
    "module": "es2015",
    "moduleResolution": "node",
    "outDir": "../dist/out-tsc",
    "sourceMap": true,
    "target": "es5",
    "typeRoots": ["../node_modules/@types"],
    "paths": {
      "gamifilearning-lib": ["../dist/gamifilearning-lib"],
      "gamifilearning-lib/*": ["../dist/gamifilearning-lib/*"],
      "@gl": ["../projects/gamifilearning-lib/src"],
      "@gl/*": ["../projects/gamifilearning-lib/src/*"]
    }
  }
}
